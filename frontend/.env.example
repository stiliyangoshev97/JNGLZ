# JunkieFun Frontend Environment Variables
# Copy this file to .env and fill in your values

# WalletConnect Project ID (REQUIRED)
# Get one at https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Chain ID (97 = BNB Testnet, 56 = BNB Mainnet)
VITE_CHAIN_ID=97

# Enable testnet chains
VITE_ENABLE_TESTNET=true

# PredictionMarket Contract Address (v3.5.0)
VITE_CONTRACT_ADDRESS=0x8e6c4437CAE7b9B78C593778cCfBD7C595Ce74a8

# The Graph Gateway URL (v3.4.2 - Published, higher rate limits)
# For development, you can use Studio URL: https://api.studio.thegraph.com/query/1722665/junkiefun-bnb-testnet/3.4.2
VITE_SUBGRAPH_URL=https://gateway.thegraph.com/api/subgraphs/id/21Mbjuj7SdV8YmHYaZ56Z17hVSgJBBgcDkKFceNjeDpn

# The Graph API Key (required for gateway URL)
VITE_GRAPH_API_KEY=your_graph_api_key_here

# Admin Addresses (comma-separated, MultiSig signers)
VITE_ADMIN_ADDRESSES=0x4Cca77ba15B0D85d7B733E0838a429E7bEF42DD2,0xC119B9152afcC5f40C019aABd78A312d37C63926,0x6499fe8016cE2C2d3a21d08c3016345Edf3467F1
