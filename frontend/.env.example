# JunkieFun Frontend Environment Variables
# Copy this file to .env and fill in your values

# ==============================================================================
# NETWORK SWITCH (Change ONE variable to switch networks!)
# ==============================================================================
# true = BNB Testnet (Chain ID: 97)
# false = BNB Mainnet (Chain ID: 56)
VITE_IS_TESTNET=true

# ==============================================================================
# TESTNET CONFIGURATION (used when VITE_IS_TESTNET=true)
# ==============================================================================
# PredictionMarket Contract Address (Testnet v3.8.3)
VITE_TESTNET_CONTRACT_ADDRESS=0xC97FB434B79e6c643e0320fa802B515CedBA95Bf

# The Graph Gateway URL (jnglz-testnet-fresh v5.2.0)
VITE_TESTNET_SUBGRAPH_URL=https://gateway.thegraph.com/api/subgraphs/id/3XxbwnAdLjsRWR3DeKJFbjjnahwMuDiG5H5qMuriDGcC

# The Graph API Key (Testnet Account)
VITE_TESTNET_GRAPH_API_KEY=your_testnet_graph_api_key_here

# Admin Addresses (Testnet MultiSig signers)
VITE_TESTNET_ADMIN_ADDRESSES=0x4Cca77ba15B0D85d7B733E0838a429E7bEF42DD2,0xC119B9152afcC5f40C019aABd78A312d37C63926,0x6499fe8016cE2C2d3a21d08c3016345Edf3467F1

# ==============================================================================
# MAINNET CONFIGURATION (used when VITE_IS_TESTNET=false)
# ==============================================================================
# PredictionMarket Contract Address (Mainnet v3.8.3)
VITE_MAINNET_CONTRACT_ADDRESS=0xA482Ac7acbf846F2DAEE8b8dF3D7e77F85CC7528

# The Graph Gateway URL (jnglz-mainnet)
VITE_MAINNET_SUBGRAPH_URL=

# The Graph API Key (Mainnet Account)
VITE_MAINNET_GRAPH_API_KEY=your_mainnet_graph_api_key_here

# Admin Addresses (Mainnet MultiSig signers)
VITE_MAINNET_ADMIN_ADDRESSES=0x89b1163c90c22775ea45F69607d933618ccFCE3C,0x841528d507D91eabB924b48A45E32edC8A723f4b,0x792C04Eea5D788D2B5C22F76F5C33a982f786116

# ==============================================================================
# SHARED CONFIGURATION (used for both networks)
# ==============================================================================
# WalletConnect Project ID (REQUIRED)
# Get one at https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Social Links
VITE_X_URL=https://x.com/jnglzdotfun
VITE_TELEGRAM_URL=https://t.me/jnglzdotfun

# Maintenance Mode (optional)
# Set to "true" to block the entire site with a maintenance page
# VITE_MAINTENANCE_MODE=false

# Maintenance Message (optional, custom message to display)
# VITE_MAINTENANCE_MESSAGE=We're upgrading our systems to serve you better!

# Maintenance End Time (optional, expected time to be back)
# VITE_MAINTENANCE_END_TIME=January 20, 2026 at 10:00 UTC
